{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "analticsetldatafactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ETLOrchestration')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "s_Import_Control",
						"type": "SqlServerStoredProcedure",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"storedProcedureName": "[concat('[', 'Insight].[s_Import_Control]')]",
							"storedProcedureParameters": {
								"PathName": {
									"value": "blobs",
									"type": "String"
								},
								"TableName": {
									"value": "All",
									"type": "String"
								},
								"ReCreateTables": {
									"value": "false",
									"type": "Boolean"
								},
								"TableType": {
									"value": "regular",
									"type": "String"
								},
								"SystemTablesExist": {
									"value": "0",
									"type": "Int32"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "AzureSqlDbLinkedService",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "s_T24ConsolKeys_Add",
						"type": "SqlServerStoredProcedure",
						"dependsOn": [
							{
								"activity": "s_Import_Control",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"storedProcedureName": "[concat('[', 'Insight].[s_T24ConsolKeys_Add]')]",
							"storedProcedureParameters": {
								"TableName": {
									"value": "All",
									"type": "String"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "AzureSqlDbLinkedService",
							"type": "LinkedServiceReference"
						}
					}
				]
			}
		},
		{
			"name": "[concat(parameters('factoryName'), '/Operations')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Run Processes",
						"type": "SqlServerStoredProcedure",
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"typeProperties": {
							"storedProcedureName": "ARunDataOperations"
						},
						"linkedServiceName": {
							"referenceName": "AzureSqlDatabase1",
							"type": "LinkedServiceReference"
						}
					}
				]
			}
		}
	]
}